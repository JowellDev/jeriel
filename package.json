{
  "name": "jeriel",
  "private": true,
  "sideEffects": false,
  "packageManager": "pnpm@10.12.4",
  "scripts": {
    "build": "run-s 'build:*'",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=cjs ./server.ts --outdir=build --bundle --external:fsevents",
    "dev": "run-p quirrel 'dev:*'",
    "dev:server": "cross-env NODE_ENV=development npm run build:server -- --watch",
    "dev:remix": "remix dev --manual -c \"node ./build/server.js\"",
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "quirrel": "quirrel",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "typecheck": "tsc --noEmit",
    "migration:create": "pnpm prisma migrate dev --name",
    "db:setup": "prisma migrate reset --force --skip-seed && prisma db push",
    "db:generate": "prisma generate",
    "db:seed": "prisma db seed",
    "db:push": "prisma db push",
    "db:deploy": "prisma migrate deploy",
    "seed:prod": "node scripts/seed.mjs"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/postgres-data"
  ],
